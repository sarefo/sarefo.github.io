{
  "name": "duonat",
  "version": "1.5.28",
  "author": "Stefan Obenauer",
  "description": "DuoNat - iNat Taxonomy Trainer",
  "main": "index.html",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "mongodb": "^6.8.1",
    "vis-network": "^9.1.2"
  },
  "scripts": {
    "start": "echo 'Add a start script if needed'",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "babel-jest": "^29.6.2",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "transform": {},
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "mjs"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/__tests__/**/*.mjs",
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "**/?(*.)+(spec|test).mjs"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  }
}
